package com.fundamental.model.dto;

/**
 * @author Henry Barrientos
 */
public class DtoPrecio {

    private Integer id;
    private Integer productoId;
    private String productoName;
    private Double precioAS;
    private Double precioSC;
    //Adicionales
    private Integer tipoDespachoId;
    private String tipoDespachoName;

    public DtoPrecio(Integer id, String productoName, Double precioAS, Double precioSC) {
        this.id = id;
        this.productoName = productoName;
        this.precioAS = precioAS;
        this.precioSC = precioSC;
    }

    public DtoPrecio(Integer id, Integer productoId, String productoName, Integer tipoDespachoId, String tipoDespachoName) {
        this.id = id;
        this.productoId = productoId;
        this.productoName = productoName;
        this.tipoDespachoId = tipoDespachoId;
        this.tipoDespachoName = tipoDespachoName;
    }

    public Integer getId() {
        return id;
    }

    public void setId(Integer id) {
        this.id = id;
    }

    public String getProductoName() {
        return productoName;
    }

    public void setProductoName(String productoName) {
        this.productoName = productoName;
    }

    public Double getPrecioAS() {
        return precioAS;
    }

    public void setPrecioAS(Double precioAS) {
        this.precioAS = precioAS;
    }

    public Double getPrecioSC() {
        return precioSC;
    }

    public void setPrecioSC(Double precioSC) {
        this.precioSC = precioSC;
    }

    public String getTipoDespachoName() {
        return tipoDespachoName;
    }

    public void setTipoDespachoName(String tipoDespachoName) {
        this.tipoDespachoName = tipoDespachoName;
    }

    public Integer getProductoId() {
        return productoId;
    }

    public void setProductoId(Integer productoId) {
        this.productoId = productoId;
    }

    public Integer getTipoDespachoId() {
        return tipoDespachoId;
    }

    public void setTipoDespachoId(Integer tipoDespachoId) {
        this.tipoDespachoId = tipoDespachoId;
    }
}
